<div id="<%= dom_id home %>">
  <p>
    <strong>Image:</strong>
    <%= image_tag home.image %> 
  </p>
  <p>
    <strong>Year built:</strong>
    <%= home.year_built %>
  </p>

  <p>
    <strong>Size:</strong>
    <%= home.size %> sq. ft
  </p>

  <p>
    <strong>Price:</strong>
   $<%= home.price %>
  </p>

  <p>
    <strong>Listing date:</strong>
    <%= home.listing_date %>
  </p>

  <p>
    <strong>Bedrooms:</strong>
    <%= home.bedrooms %>
  </p>

  <p>
    <strong>Baths:</strong>
    <%= home.baths %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= home.address %>
  </p>

  <p>
    <strong>State:</strong>
    <%= home.state %>
  </p>

  <p>
    <strong>City:</strong>
    <%= home.city %>
  </p>
</div>

